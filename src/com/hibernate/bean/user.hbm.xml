<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hibernate.bean.User" table="ce_copy">
        <id column="ID" name="id" type="java.lang.Integer">
        <generator class="increment"></generator>  
        </id>
        <property column="fname" name="fname" type="string" />
        <property column="lname" name="lname" type="string" />
        <property column="avl" name="avl" type="string" />
        <property column="total" name="total" type="string" />
        <property column="unrealized" name="unrealized" type="string" />
        <property column="cash" name="cash" type="string" />
    </class>
</hibernate-mapping>
